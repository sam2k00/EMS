@{
}

@model List<Voter>

<div>
    <h1>
        Voters List
    </h1>
</div>

<div>
	<table class="table table-bordered">
		<thead>
			<tr>
				<th>VoterId</th>
				<th>Name</th>
				<th>Address</th>
				<th>Status</th>
				<th>StateId</th>
				<th>Action</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in @Model)
			{
				<tr>
					<th scope="row">@item.VoterId</th>
					<td>@item.Name</td>
					<td>@item.Address</td>
					<td>@item.Status</td>
					<td>@item.StateId</td>
					<td>
						<a data-value="@item.VoterId" href="javascript:void(0)" class="btnEdit">Edit</a> |
						<a data-value="@item.VoterId" href="javascript:void(0)" class="btnDelete">Delete</a>
					</td>
				</tr>
			}
		</tbody>
	</table>
</div>