@{
}
<div>
    <h1>Parties</h1>
</div>

@model List<Partie>

<div>
	<table class="table table-bordered">
		<thead>
			<tr>
				<th>PartyId</th>
				<th>PartyName</th>
				<th>SymbolId</th>
				<th>Action</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in @Model)
			{
				<tr>
					<th scope="row">@item.PartyId</th>
					<td>@item.PartyName</td>
					<td>@item.SymbolId
						@item.PartiSymbol.SymbolName
					</td>
					<td>
						<a data-value="@item.PartyId" href="javascript:void(0)" class="btnEdit">Edit</a> |
						<a data-value="@item.PartyId" href="javascript:void(0)" class="btnDelete">Delete</a>

						

					</td>
				</tr>
			}
		</tbody>
	</table>
</div>
