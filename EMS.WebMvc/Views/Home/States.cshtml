@{
}

<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>

@model List<States>

	<div>
		<h1>
		States
		</h1>
	</div>
<div>
	<table class="table table-bordered">
		<thead>
			<tr>
				<th>ID</th>
				<th>Name</th>
				<th>MpSeats</th>
				<th>Action</th>
			</tr>
		</thead>
		<tbody>
			@foreach (var item in @Model)
			{
				<tr>
					<th scope="row">@item.StateId</th>
					<td>@item.StateName</td>
					<td>@item.MpSeats</td>
					<td>
						<a data-value="@item.StateId" href="javascript:void(0)" class="btnEdit">Edit</a> |
						<a data-value="@item.StateId" href="javascript:void(0)" class="btnDelete">Delete</a>
					</td>
				</tr>
			}
		</tbody>
	</table>
</div>